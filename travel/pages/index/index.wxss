page, .wrap {
  width: 100%;
  height: 100%;
}

.wrap {
  display: flex;
  flex-direction: column;
  background: #F5F5F5;
}

.top {
  width: 100%;
  height: 350rpx;
  background: #fff;
}

.top image {
  width: 100%;
  height: 100%;
}

.box {
 padding-top:50rpx;
 box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  background: #fff;
}

.box view {
  width: 40%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border: 1px #9e9e9e solid;
  margin-bottom: 40rpx;
  border-radius: 10rpx;
}

.nav {
  display: flex;
  justify-content: space-around;
  background: #fff;
  padding-bottom: 80rpx;
  box-sizing: border-box;
}

.nav_box {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #4b4b4b;
  font-size: 35rpx;
}

.nav_box image {
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 15rpx;
}
.footer{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 50rpx;
}
.footer image{
  width: 60rpx;
  height: 60rpx;
}
.content{
  flex: 1;
  overflow-y: auto;
}
.footer view{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items:center;
}
.title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px #eee solid;
  text-align: center;
  margin-top: 30rpx;
  background: #fff;
}
.list{
  padding: 0 20rpx;
  box-sizing: border-box;
}
.child_box{
  margin-bottom: 40rpx;
}
.child_box image{
  width: 100%;
  border-radius: 10rpx;
}
.introduce{
  font-size: 35rpx;
  font-weight: bold;
}